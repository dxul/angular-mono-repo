{
  "packages": [
    "projects/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "independent",
  "command": {
    "version": {
      "allowBranch": "master",
      "ignoreChanges": [
        "**/package.json",
        "**/__tests__/**",
        "**/*.md",
        "**/Podfile.lock",
        "**/yarn.lock",
        "**/.vscode/**",
        "**/.vscode/**"
      ]
    }
  }
}
